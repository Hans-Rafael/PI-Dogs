{
    "functions": {
      "api/app.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "client/dist"
      },
      {
        "src": "/.*",
        "dest": "api/app.js"
      }
    ]
  }